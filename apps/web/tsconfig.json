{
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "./",
    "composite": false,
    "inlineSources": false,
    "preserveWatchOutput": true,
    "strict": true,
    "module": "CommonJS",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "pretty": true,
    "noEmitOnError": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "strictPropertyInitialization": false,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "paths": {
      "react": [
        "../../node_modules/@types/react"
      ],
      "@typings/*": [
        "typings/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@stores/*": [
        "stores/*"
      ],
      "@common/*": [
        "common/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@editor/*": [
        "editor/*"
      ],
      "@modals/*": [
        "modals/*"
      ],
      "@drawers/*": [
        "drawers/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  }
}